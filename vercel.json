{
  "buildCommand": "echo 'No build needed - static site'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "headers": [
    {
      "source": "/app.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}